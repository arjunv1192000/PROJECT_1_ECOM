<div class="catpage" id="footer">
    <h2>Category Edit</h2>

    <form action="/admin/category-edit_submit/<%= data._id%>" method="post">
        <div class="row mb-3">
            <label for="inputEmail" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-5">
                <input type="text" name="name" class="form-control" value="<%= data.name %>" >

            </div>
            <div class="col-sm-5">
                <button class="btn btn-secondary" type="submit" id="my-button" onclick="myFunction(this.id)">submit</button><br>

            </div>
        </div>
        <p class="error"></p>
    </form>
</div>