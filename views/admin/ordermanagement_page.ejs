<div class="catpage" id="footer">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Date</th>
                <th scope="col">Address</th>
                <th scope="col">Pincode</th>
                <th scope="col">phone</th>
                <th scope="col">payment</th>
                <th scope="col">Totalprice</th>
                <th scope="col">status</th>
                <th scope="col">Products</th>
            </tr>
        </thead>
        <tbody>
         <% Allorder.forEach(function(data, i){ %>
            <tr>
                <td><%= data.deliveryDetails.Name %></td>
                <td><%= data.date %></td>
                <td><%= data.deliveryDetails.address%></td>
                <td><%= data.deliveryDetails.Postcode %></td> 
                <td><%= data.deliveryDetails.phone  %></td>
                <td><%= data.paymentMethod %></td>
                <td><%= data.totaAmount %></td>
                <td>

                    <%= data.status %>

                    <form action="/admin/cancelorders/<%= data._id %>" method="post">
                        <input name="status" value="<%= data.status %>" hidden>
                          <button type="submit" class="btn btn-danger btn-sm" >Cancel</button>
                           
                      </form>

                </td>
                <td>
                    <a href="/admin/orderproduct/<%= data._id %>"><button class="btn btn-success" id="my-button" >Products</button></a>
                </td>
            </tr>
         <% }); %>
      </tbody>
    </table>
</div>